a.tc-tiddlylink-missingXXX {
  color: red;
}

.tc-story-results {
  width: 49%;
  border: 1px solid #cccccc;
  padding: 3px 3px 3px 3px;
}

.tc-btn-mini{
  vertical-align: middle;
}

.tc-btn-mini .tc-image-button{
  width: 1em;
  height: 1em;
  margin-right: 3px;
}

.tc-story-editor {
  width: 100%;
}

.tc-fr {
  float: right;
}

.tc-fl {
  float: left;
}


.clearfix { display:block;} 

.clearfix:after {
     visibility: hidden;
     display: block;
     font-size: 0;
     content: " ";
     clear: both;
     height: 0;
}