{
  "name": "pluginlibrary-test-server",
  "version": "0.0.1",
  "description": "tiddlywiki edition development helper",
  "main": "",
  "scripts": {
    "start": "tiddlywiki . --listen",

    "pretest": "npm run build:test:plugins",
    "test": "npm run build && npm start",

    "build:test:plugins": "tiddlywiki ../pluginlibraryWL --output ../pluginlibraryWL-server/files/local/library/latest --build library",

    "build:plugins": "tiddlywiki ../pluginlibraryWL --build library",

    "prebuild": "npm run build:plugins",
    "build": "tiddlywiki ../pluginlibraryWL --build index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wikilabs/plugins"
  },
  "keywords": [
    "tiddlywiki",
    "libraries"
  ],
  "author": "Mario Pietsch",
  "license": "BSD 3 clause",
  "bugs": {
    "url": "https://github.com/wikilabs/plugins/issues"
  },
  "homepage": "https://wikilabs.github.io/editions/pluginlibraryWL",
  "dependencies": {
  },
  "devDependencies": {
    "tiddlywiki": "^5.1.0"
  }
}
